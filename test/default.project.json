{
	"name": "test",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"TS": {
				"$className": "Folder",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				},
				"include": {
					"$path": "out/include"
				},
				"packages": {
					"$path": "out/packages"
				}
			}
		},
		"TestService": {
			"$properties": {
				"ExecuteWithStudioRun": true,
				"Timeout": 30
			},
			"$path": "out/test"
		}
	}
}
