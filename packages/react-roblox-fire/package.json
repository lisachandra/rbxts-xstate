{
	"name": "@rbxts/react-roblox-fire",
	"version": "0.0.1",
	"description": "Fires react events by patching react-roblox. (Ported to Roblox-TS)",
	"main": "out/init.luau",
	"types": "out/index.d.ts",
	"files": [
		"out",
		"default.project.json",
		"!**/*.tsbuildinfo",
		"!**/__tests__"
	],
	"scripts": {
		"build": "shx rm -rf out && rbxtsc --verbose --allowCommentDirectives",
		"dev": "shx rm -rf out && rbxtsc -w --allowCommentDirectives"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lisachandra/rbxts-jsdotlua.git"
	},
	"author": "Kampfkarren",
